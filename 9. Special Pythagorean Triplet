output = []
for a in range(1, 1000):
    for b in range(a, 1000):
        c = (a*2 + b*2)*0.5
        if c == int(c):
            output.append((a, b, int(c)))

maxim = []
for _ in range(len(output)):
    if sum(output[_]) == 1000:
        prod = output[_][0]*output[_][1]*output[_][2]
        maxim.append(prod)
print(max(maxim))
